#!/usr/bin/python

import sys, socket
from time import sleep
import struct

TARGET = 'ip_address'
PORT = 1234
PATH = '/'

CRASH_LEN = 0
OVERFLOW_LEN = 0

NOP_SLED = "\x90" * 32

